# Auto detect text files and perform LF normalization
* text=auto

# Talend files
*.item       filter=lfs diff=lfs merge=lfs -text
*.properties filter=lfs diff=lfs merge=lfs -text
*.project    filter=lfs diff=lfs merge=lfs -text
*.jar        filter=lfs diff=lfs merge=lfs -text
*.class      filter=lfs diff=lfs merge=lfs -text
*.java       filter=lfs diff=lfs merge=lfs -text
*.lst        filter=lfs diff=lfs merge=lfs -text
*.xml        filter=lfs diff=lfs merge=lfs -text
*.xsd        filter=lfs diff=lfs merge=lfs -text
*.index      filter=lfs diff=lfs merge=lfs -text
*.prefs      filter=lfs diff=lfs merge=lfs -text
*.settings   filter=lfs diff=lfs merge=lfs -text
*.log        filter=lfs diff=lfs merge=lfs -text
*.data       filter=lfs diff=lfs merge=lfs -text
*.tmp        filter=lfs diff=lfs merge=lfs -text
*.ser        filter=lfs diff=lfs merge=lfs -text
*.si         filter=lfs diff=lfs merge=lfs -text
*.cfs        filter=lfs diff=lfs merge=lfs -text
*.txt        filter=lfs diff=lfs merge=lfs -text
*.dat        filter=lfs diff=lfs merge=lfs -text
*.xmi        filter=lfs diff=lfs merge=lfs -text
*.snap       filter=lfs diff=lfs merge=lfs -text
*.version    filter=lfs diff=lfs merge=lfs -text
